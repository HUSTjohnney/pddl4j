PDDLTaskNetwork task_network() : // ADD HDDL
{
    Expression<String> tasks = null;
    Expression<String> ordering = new Expression<String>(PDDLConnective.AND);
    Expression<String> constraints = new Expression<String>(PDDLConnective.AND);
    PDDLTaskNetwork tn = null;
}
{
    LOOKAHEAD (1)
        (<TASKS> | <SUBTASKS>)
        tasks = subtask_def()
   	    [<ORDERING> ordering = htn_ordering_constraints_def()]
   	    [<CONSTRAINTS> constraints = htn_constraints_def()]
   	    {
