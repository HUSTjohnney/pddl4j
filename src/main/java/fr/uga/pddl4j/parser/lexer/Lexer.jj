}
{
	(	LOOKAHEAD(2)
			exp=and_pref_con_gd()
	|	LOOKAHEAD(2, {this.isRequirement(PDDLRequireKey.UNIVERSAL_PRECONDITIONS)})
			exp=forall_pref_con_gd()
	| 	LOOKAHEAD(2, {this.isRequirement(PDDLRequireKey.PREFERENCES)})
			exp=named_pref_con_gd()
	|	LOOKAHEAD(2)
			exp=con_gd()
	) {
		return exp;
	}
}

