    | LOOKAHEAD (1)
        (<ORDERED_TASKS> | <ORDERED_SUBTASKS>)
        tasks = subtask_def()
        [<ORDERING> ordering = htn_ordering_constraints_def()]
        [<CONSTRAINTS> constraints = htn_constraints_def()]
        {
            return new PDDLTaskNetwork(tasks, ordering, constraints, true);
        }
